{
  "name": "private-web",
  "version": "0.0.0",
  "description": "A secure private dashboard for personal use.",
  "private": true,
  "author": "piku-afk",
  "engines": {
    "node": "=22.15.0"
  },
  "packageManager": "pnpm@10.10.0",
  "type": "module",
  "scripts": {
    "format": "pnpm exec biome format --write",
