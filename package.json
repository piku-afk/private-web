    "react-router": "^7.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "netlify-cli": "^23.0.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "supabase": "^2.33.9",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
